<script>
    $('#careerBtn').click(function () {
        careerBtn_validation();
    });

    function careerBtn_validation() {
        debugger;
        var err = 0;
        if ($('#careername').val() == 0) {
            alert("Insert value");
            err++;
        }
        if ($('#careerPhone').val() == 0) {
            alert("Insert value");
            err++;
        }
        if ($('#careerMail').val() == 0) {
            alert("Insert value");
            err++;
        }

        //if (($('input[name="chkNew"]').length > 0) && ($('input[name="chkNew"]:checked').val() == undefined || $('input[name="chkNew"]:checked').val() == "")) {
        //    $("#tblError").html("Please select at least one supplier.").css('display', 'inline-block');
        //    err++;
        //}

        if (err == 0) {
            SubmitResume();
            alert('Your resume added succesfully........');
        }
        else {
            alert('Problem adding the comments...');
        }
    }

    function SubmitResume() {
        debugger;
        var Career = {
            Designation: $("input[name=talente]:checked").val(),
            FullName: $('#careername').val(),
            PhoneNo: $('#careerPhone').val(),
            EmailID: $('#careerMail').val(),
        };
        var file_data = $("#filecv").prop("files")[0];   // Getting the properties of file from file field
        var form_data = new FormData();                  // Creating object of FormData class
        form_data.append("file", file_data)              // Appending parameter named file with properties of file_field to form_data
        form_data.append("Designation", Career.Designation)
        form_data.append("FullName", Career.FullName)
        form_data.append("PhoneNo", Career.PhoneNo)
        form_data.append("EmailID", Career.EmailID)
        

        $.ajax({
            url: '@Url.Action("CareerPost", "Home")',
            type: "POST",
            dataType: 'json',
            data: form_data.serialize(),
            async: false,
            cache: false,
            traditional: true,
            contentType: 'application/json',
            success: function (data) {
                window.location.href = '@Url.Action("Index", "Home")';
            },
            error: function () {
                alert("Sorry");
            }
        });
    };
</script>