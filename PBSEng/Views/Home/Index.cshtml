    <!-- EVENT -->
    <section class="event-calender-main ">
        <div class="container">
            <div class="page-header">
                <h2 class="hfont"> <em>Event</em> Calander <span class="head-blk" style="color:#656565;">Check our upcoming big events</span></h2>
            </div>
            <ul class="timeline">
                @{
                    int c = 0;
                    foreach (var item in ViewData["Events"] as List<Event>)
                    {
                        c++;
                        if (c % 2 == 0)
                        {
                            <li class="timeline-inverted wow fadeInUp">
                                <div class="timeline-badge"></div>
                                <div class="timeline-panel">
                                    <div class="timeline-heading">
                                        <h4 class="timeline-title">@item.Title</h4>
                                        <p><small class="text-muted"> @item._Date.ToString("dd/MM/yyyy")</small></p>
                                    </div>
                                    <div class="timeline-body">
                                        <p>@item.Description</p>
                                        <a target="_blank" href="@item.Location_URL"><span>Address</span> <small>@item.Location</small></a>
                                    </div>
                                </div>
                            </li>
                        }
                        else
                        {
                            <li class=" wow fadeInUp">
                                <div class="timeline-badge"></div>
                                <div class="timeline-panel">
                                    <div class="timeline-heading">
                                        <h4 class="timeline-title">@item.Title</h4>
                                        <p><small class="text-muted"> @item._Date.ToString("dd/MM/yyyy")</small></p>
                                    </div>
                                    <div class="timeline-body">
                                        <p>@item.Description</p>
                                        <a target="_blank" href="@Html.Raw(Html.AttributeEncode(@item.Location_URL))"><span>Address</span> <small>@item.Location</small></a>
                                    </div>
                                </div>
                            </li>
                        }
                    }

                }
            </ul>
        </div>
    </section>   

