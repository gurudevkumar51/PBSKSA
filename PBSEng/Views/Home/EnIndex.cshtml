    <script>
        $(window).load(function () {
            $(".loading-area").fadeOut();
        });

        $(document).ready(function () {

            $(".barsmob").click(function () {
                //$(".responsive-menu").attr('data-check','open');
                var checkAttr = $(".responsive-menu").attr('data-check');
                if (checkAttr == 'open') {
                    $(".responsive-menu").attr('data-check', 'close');
                    $(".responsive-menu").css("height", "50px");
                }
                else {
                    $(".responsive-menu").attr('data-check', 'open');
                    $(".responsive-menu").css("height", "350px");
                }

            });

            $(window).scroll(function () {
                if ($(this).scrollTop() > 100) {
                    $('.brand').fadeOut(250, function () {
                        $('.nav-bar-main').fadeIn(250);
                    });
                } else {
                    $('.nav-bar-main').fadeOut(250, function () {
                        $('.brand').fadeIn(250);
                    });
                }
            });

            $('.scroll-top').click(function () {
                $('html, body').animate({ scrollTop: 0 }, 800);
                return false;
            });

            $(".nav-bar-main ul li a , .brand .nav-bar-slide ul li a , .tog li a").click(function () {
                var h = $(this).attr('href');
                $('html, body').animate({ scrollTop: ($(h).offset().top) - 50 }, '800');
                return false;
            });


            $('#myTab').tabCollapse();

            $('.bars,.overlay').click(function () {
                $("body").toggleClass("sh");
            });

            new WOW().init();

        });

    </script>
